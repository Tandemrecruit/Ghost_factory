# API Keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Discord Webhook (Optional - for build notifications)
# Get your webhook URL from Discord: Server Settings > Integrations > Webhooks > New Webhook
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR_WEBHOOK_ID/YOUR_WEBHOOK_TOKEN

# Metrics Tracking (Optional - for page view and conversion analytics)
# Set to 'true' to enable metrics collection
GF_METRICS_ENABLED=true
# Optional: Webhook URL to send metrics events to an external receiver
# When not set: events are logged in development, no-op in production
GF_METRICS_WEBHOOK_URL=
# Optional: Secret for authenticating webhook requests (sent as Bearer token)
GF_METRICS_WEBHOOK_SECRET=
